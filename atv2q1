/// QUESTAO 1 ///

public class Paciente
{
   int codigo;
    String nome;
    String datanasc;
    String sexo;
    String psaude;
    String alergia;
    String tiposang;
    
    public int getCodigo() {
        return this.codigo;
    }
    
    public String getNome() {
        return this.nome;
    } 
    
    public String getDatanasc() {
        return this.datanasc;
    }
    
    public String getSexo() {
        return this.sexo;
    }
    
    public String getPsaude() {
        return this.psaude;
    }
    
    public String getAlergia() {
        return this.alergia;
    }
    
    public String getTiposang() {
        return this.tiposang;
    }
}
    
    